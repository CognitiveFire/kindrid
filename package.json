{
  "name": "kindrid-prototype",
  "version": "1.0.0",
  "description": "Secure, AI-powered platform for sharing school photos and videos with full parental control",
  "main": "index.js",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview --port $PORT",
    "start": "node server.js",
    "railway-start": "RAILWAY_STARTUP_DELAY=10000 NODE_ENV=production node server.js",
    "deploy": "npm run build && git add dist -f && git commit -m 'deploy' && git subtree push --prefix dist origin gh-pages",
    "lint": "eslint . --ext js,jsx --report-unused-disable-directives --max-warnings 0"
  },
  "dependencies": {
    "clsx": "^1.2.1",
    "express": "^5.1.0",
    "lucide-react": "^0.263.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.8.0",
    "tailwind-merge": "^1.14.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@vitejs/plugin-react": "^4.0.0",
    "autoprefixer": "^10.4.14",
    "eslint": "^8.38.0",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.3.4",
    "postcss": "^8.4.23",
    "tailwindcss": "^3.3.2",
    "vite": "^4.3.0"
  },
  "keywords": [
    "school-photos",
    "parental-controls",
    "ai-powered",
    "gdpr-compliant",
    "secure-sharing"
  ],
  "author": "Kindrid Team",
  "license": "MIT"
}
